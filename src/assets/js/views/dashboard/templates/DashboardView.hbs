<!-- Verify Account -->
<form id="verfiyAccount" class="yellowVerify ">
    <label>{{VerifyAccountHeader}}</label>
    <div class="verify">
        <div class="input-group">
            <input type="text" class="form-control" id="verifyCode" placeholder="Enter Code">
            <span class="input-group-btn">
                <button class="btn btn-secondary" id="verify_code" type="button">{{Ok}}</button>
            </span>
        </div>
    </div>
    <button id="resend_btn" type="button" class="btn">{{Resend}}</button>
</form>
<div id="menuLayoutContainer"></div>
<div id="messageModal" class="modal fade stopTransfer noDisplay" role="dialog" data-backdrop="static" >
    <div class="modal-dialog">     
        <div class="modal-content">
            <div class="modal-header">
                <h4>Photogurus</h4>
            </div>
            <div class="modal-body text-center">
                <p id="stopTransferText">[text to be replaced]</p>
            </div>
            <div class="modal-footer text-center">
                <button type="button" class="btn btn-default stopTransferModal"  data-dismiss="modal">Ok</button>
            </div>

        </div>
    </div>
</div>
<div id="errorMessageModal" class="modal fade copy-link-modal" data-backdrop="static" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <h4>Photogurus</h4>
            </div>
            <div class="modal-body text-center">
                <div><strong class="copy-text">Link copied to the clipboard.</strong></div>
            </div>
            <div class="modal-footer text-center">
                <button type="button" class="btn btn-default" data-dismiss="modal" id="errorMessageModalOKBtn">Ok</button>
            </div>
        </div>
    </div>
</div>
<div id="messageModal" class="modal fade alertNoEditShare noDisplay" role="dialog" data-backdrop="static" >
    <div class="modal-dialog">     
        <div class="modal-content">
            <div class="modal-header">
