<div id="flickrModal" class="modal fade  uploaderModal facebookAlbumSelectionModal" role="dialog" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog">
        <div class="flickrphotosSelectionCount">0 photos selected</div>
        <!-- Modal content-->
        <div class="modal-content">
            <!--Modal body-->
            <div class="modal-body">
                <div class="" id="fb_modal" style="">
                    <!-- The container for the uploaded files -->
                    <div class="fb_title">
                        <div class="spriteImage modalCloseBlock modalCloseIcon" id="closeFlickrAlbumList"></div>
                        <div class="closeFBModal spriteImage modalBackArrow"> </div>
                        <span id="fbInnerTitle">Flickr Photo Album</span>
                        <div class="saveSelection saveFbPhots saveFbPhotsDisabled" id="importSelectFlickerPhotosId">Save</div>
                    </div>
                    <div class="modal-body-right">
                        <div class="selectAlbumTitile">Select an album</div>
                        <div class="selectAllFBContent">
                            <div>Select photos. Press shift to select multiple photos.</div>
                            <div class="selectAllCheckIcon spriteImage"></div>                                
                            <div class="selectAllAlbumPhots">Select All</div>
                        </div>
                        <div id="albumCollectionList" class="files"></div>
                        <div id="albumPhotosCollectionList"  class="files btn-group" data-toggle="buttons"></div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
